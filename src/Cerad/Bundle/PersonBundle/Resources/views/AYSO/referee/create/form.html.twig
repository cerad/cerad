<form 
    id     = "cerad_person_ayso_referee_form"
    class  = "cerad_person_ayso_referee_form zayso_form1" 
    action = "{{ path('cerad_person_ayso_referee_create') }}" 
    method = "post" 
    {{ form_enctype(form) }} novalidate
 >
    <fieldset><legend><span>AYSO Information</span></legend>
        {{ form_row(form.leagueAYSOVolunteer.identifier) }}
        {{ form_row(form.leagueAYSOVolunteer.league)     }}
        {{ form_row(form.certAYSOReferee.badgex)         }}
    </fieldset>
    <fieldset><legend><span>Contact Information</span></legend>
        {{ form_row(form.name)      }}
        {{ form_row(form.firstName) }}
        {{ form_row(form.lastName)  }}
        {{ form_row(form.nickName)  }}
        {{ form_row(form.email)     }}
        {{ form_row(form.phone)     }}
    </fieldset>
    <fieldset>
        <div class="zayso_form_controls">
            <span></span>
            <input type="submit" name="AYSORefereeCreate" value="Create Person" />
        </div>
    </fieldset>
<div style="display: none;">{{ form_rest(form) }}</div>
</form>
